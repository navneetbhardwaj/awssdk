{
    "name": "navneetbhardwaj/awssdk",
    "type": "library",
    "keywords": [
        "sp-api",
        "amazon",
        "sdk",
        "openapi-generator",
        "php",
        "rest"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.0",
        "aws/aws-sdk-php": "^3.228",
        "symfony/rate-limiter": "^5.4",
        "vlucas/phpdotenv": "^5.6",
        "ext-openssl": "*",
        "dallgoot/yaml": "^1.0"
    },
    "conflict": {
        "guzzlehttp/guzzle": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "autoload": {
        "psr-4": {
            "SpApi\\AuthAndAuth\\": "src/authandauth",
            "SpApi\\": "lib/",
            "SpApi\\Test\\" : "test/"
        }
    }
}
